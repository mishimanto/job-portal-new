  `updated_at` timestamp NULL DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

-- --------------------------------------------------------

--
-- Table structure for table `profile_visibility_settings`
--

CREATE TABLE `profile_visibility_settings` (
  `id` bigint(20) UNSIGNED NOT NULL,
  `user_id` bigint(20) UNSIGNED NOT NULL,
  `make_profile_public` tinyint(1) NOT NULL DEFAULT 1,
  `show_education` tinyint(1) NOT NULL DEFAULT 1,
  `show_experience` tinyint(1) NOT NULL DEFAULT 1,
  `show_projects` tinyint(1) NOT NULL DEFAULT 1,
  `show_certifications` tinyint(1) NOT NULL DEFAULT 1,
  `show_skills` tinyint(1) NOT NULL DEFAULT 1,
  `show_social_links` tinyint(1) NOT NULL DEFAULT 1,
  `show_contact_info` tinyint(1) NOT NULL DEFAULT 0,
  `created_at` timestamp NULL DEFAULT NULL,
  `updated_at` timestamp NULL DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

